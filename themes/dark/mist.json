    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5c6370"
            }
        },
        {
            "name": "Keyword",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#c96eea"
            }
        },
        {
            "name": "Type, Interface, Struct",
            "scope": ["support.type", "entity.name.type", "entity.name.type.interface", "entity.name.type.struct"],
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "name": "Function, Method",
            "scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
            "settings": {
                "foreground": "#72c6e8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variable, Identifier, Parameter",
            "scope": ["variable", "identifier", "variable.parameter"],
            "settings": {
                "foreground": "#e9d7a6"
            }
        },
        {
            "name": "Constant, Number, Boolean",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.boolean"
            ],
            "settings": {
                "foreground": "#d19a66"
            }
        },
        {
            "name": "String",
            "scope": ["string", "constant.other.symbol"],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "name": "Builtin, Package",
            "scope": ["support.function", "support.class", "support.other.package"],
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "name": "Operator",
            "scope": ["keyword.operator", "punctuation"],
            "settings": {
                "foreground": "#56b6c2"
            }
        },
        {
            "name": "Struct Field",
            "scope": ["variable.other.field"],
            "settings": {
                "foreground": "#e9d7a6"
            }
        },
        {
            "name": "Error, Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "name": "HTML Tag",
            "scope": ["entity.name.tag"],
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "name": "HTML Attribute",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#c96eea",
                "fontStyle": "italic"
            }
        },
        {
            "name": "HTML/JSX Attribute",
            "scope": ["entity.other.attribute-name", "JSXAttribute", "meta.jsx.attribute", "meta.tsx.attribute"],
            "settings": {
                "foreground": "#E5C07B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "React Component Name",
            "scope": [
                "entity.name.type.class.jsx",
                "entity.name.type.class.tsx",
                "support.class.component.jsx",
                "support.class.component.tsx"
            ],
            "settings": {
                "foreground": "#E5C07B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS/SCSS Class Attribute Name",
            "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.class.scss"],
            "settings": {
                "foreground": "#c96eea"
            }
        },
        {
            "name": "CSS/SCSS Property Value",
            "scope": ["support.constant.property-value.css", "support.constant.property-value.scss"],
            "settings": {
                "foreground": "#FFB454"
            }
        },
        {
            "name": "CSS/SCSS Property Name",
            "scope": ["support.type.property-name.css", "support.type.property-name.scss"],
            "settings": {
                "foreground": "#72c6e8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "HTML Derivative",
            "scope": ["text.html.derivative"],
            "settings": {
                "foreground": "#ffffff85",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Headings Markup",
            "scope": [
                "punctuation.definition.heading.markdown",
                "markup.heading.markdown",
                "markup.italic.markdown",
                "markup.inline.raw.markdown"
            ],
            "settings": {
                "foreground": "#e06c75",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Headings Entity Markup",
            "scope": ["entity.name.section.markdown"],
            "settings": {
                "foreground": "#98C379"
            }
        },
        {
            "name": "Markdown Headings Entity Markup",
            "scope": [
                "variable.language.this.tsx",
                "variable.language.this.jsx",
                "variable.language.this.ts",
                "variable.language.this.js",
                "punctuation.accessor.tsx",
                "punctuation.accessor.jsx",
                "punctuation.accessor.ts",
                "punctuation.accessor.js"
            ],
            "settings": {
                "foreground": "#98C379"
            }
        }
    ]
}

    "version": "1.2.0",
    "private": true,
    "license": "MIT",
    "engines": {
        "vscode": "^1.95.0"
    },
    "publisher": "aklesky",
    "icon": "icons/aklesky.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/aklesky/aklesky-theme.git"
    },
    "categories": [
